    %delay 10000, 0;
    %load/vec4 v0000025c9d451280_0;
    %load/vec4 v0000025c9d405d00_0;
    %cmp/ne;
    %jmp/1 T_0.2, 4;
    %flag_mov 8, 4;
    %load/vec4 v0000025c9d4511e0_0;
    %load/vec4 v0000025c9d405c60_0;
    %cmp/ne;
    %flag_or 4, 8;
T_0.2;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 31 "$display", "FAIL: %b + %b cin &b != &b carry %b (got %b carry %b)", v0000025c9d3d2db0_0, v0000025c9d405b20_0, v0000025c9d405bc0_0, v0000025c9d405d00_0, v0000025c9d405c60_0, v0000025c9d451280_0, v0000025c9d4511e0_0 {0 0 0};
    %vpi_call 2 32 "$fatal" {0 0 0};
T_0.0 ;
    %end;
