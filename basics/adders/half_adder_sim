 .timescale -9 -12;
v000001c3c03638d0_0 .var "a", 0 0;
v000001c3c0363970_0 .var "b", 0 0;
v000001c3c0363a10_0 .net "carry", 0 0, L_000001c3c0332b90;  1 drivers
v000001c3c0363ab0_0 .net "sum", 0 0, L_000001c3c024b650;  1 drivers
S_000001c3c0365890 .scope task, "test_half_adder" "test_half_adder" 2 17, 2 17 0, S_000001c3c0365700;
 .timescale -9 -12;
v000001c3c0332db0_0 .var "a_t", 0 0;
v000001c3c024bd40_0 .var "b_t", 0 0;
v000001c3c0332910_0 .var "expected_carry", 0 0;
v000001c3c024b5b0_0 .var "expected_sum", 0 0;
TD_half_adder_tb.test_half_adder ;
    %load/vec4 v000001c3c0332db0_0;
    %store/vec4 v000001c3c03638d0_0, 0, 1;
    %load/vec4 v000001c3c024bd40_0;
    %store/vec4 v000001c3c0363970_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v000001c3c0363ab0_0;
    %load/vec4 v000001c3c024b5b0_0;
    %cmp/ne;
    %jmp/1 T_0.2, 4;
    %flag_mov 8, 4;
    %load/vec4 v000001c3c0363a10_0;
    %load/vec4 v000001c3c0332910_0;
    %cmp/ne;
    %flag_or 4, 8;
T_0.2;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 2 28 "$display", "FAIL: %b + %b != &b carry %b (got %b carry %b)", v000001c3c0332db0_0, v000001c3c024bd40_0, v000001c3c024b5b0_0, v000001c3c0332910_0, v000001c3c0363ab0_0, v000001c3c0363a10_0 {0 0 0};
    %vpi_call 2 29 "$fatal" {0 0 0};
T_0.0 ;
    %end;
S_000001c3c0363740 .scope module, "uut" "half_adder" 2 10, 3 3 0, S_000001c3c0365700;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "sum";
    .port_info 3 /OUTPUT 1 "carry";
L_000001c3c024b650 .functor XOR 1, v000001c3c03638d0_0, v000001c3c0363970_0, C4<0>, C4<0>;
L_000001c3c0332b90 .functor AND 1, v000001c3c03638d0_0, v000001c3c0363970_0, C4<1>, C4<1>;
v000001c3c024ba30_0 .net "a", 0 0, v000001c3c03638d0_0;  1 drivers
v000001c3c024bad0_0 .net "b", 0 0, v000001c3c0363970_0;  1 drivers
